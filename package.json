{
  "name": "weekly-team-reports-react",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@auth0/auth0-react": "^1.8.0",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.24.0",
    "bootstrap": "^5.1.3",
    "bootstrap-daterangepicker": "^3.1.0",
    "effector": "^22.1.2",
    "effector-react": "^22.0.6",
    "emailjs-com": "^3.2.0",
    "formik": "^2.2.9",
    "history": "^5.1.0",
    "jquery": "^3.6.0",
    "moment": "^2.29.2",
    "prop-types": "latest",
    "react": "^17.0.2",
    "react-bootstrap-daterangepicker": "^7.0.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hooks-use-modal": "^2.1.0",
    "react-refresh": "^0.9.0",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "react-tag-input": "^6.7.3",
    "web-vitals": "^1.1.2",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:development": "env-cmd -f .env.development npm run-script build",
    "build:production": "env-cmd -f .env.production npm run-script build",
    "test": "react-scripts test --passWithNoTests",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.4.4",
    "@storybook/addon-essentials": "^6.4.4",
    "@storybook/addon-links": "^6.4.4",
    "@storybook/node-logger": "^6.4.4",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.4.4",
    "babel-loader": "8.1.0",
    "bootstrap-icons": "^1.7.0",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "jest": "^26.6.0",
    "prettier": "^2.4.1",
    "sass": "^1.44.0",
    "webpack": "4.44.2",
    "webpack-cli": "^4.9.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!<rootDir>/node_modules/",
      "!src/**/*.stories.jsx",
      "!src/stub-data/**"
    ]
  }
}
